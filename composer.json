{
    "name": "jmleroux/jml-rbac-zdb",
    "description": "Zend providers for ZfcRbac module",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2",
        "rbac"
    ],
    "homepage": "http://www.github.com/jmleroux/JmlRbacZdb/",
    "authors": [
        {
            "name": "JM Leroux",
            "email": "jmleroux.pro@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jmleroux/ZfcRbac.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zf-commons/zfc-rbac": "dev-develop",
        "zfr/rbac": "~1.2",
        "zendframework/zend-db": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "zendframework/zend-form": "~2.3",
        "zendframework/zend-loader": "~2.3",
        "zendframework/zend-log": "~2",
        "zendframework/zend-modulemanager": "~2.3",
        "zendframework/zend-mvc": "~2",
        "zendframework/zend-serializer": "~2",
        "zendframework/zend-view": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "JmlRbacZdb\\": "src/"
        }
    }
}
